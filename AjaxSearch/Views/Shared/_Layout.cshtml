<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>AjaxSearch | @ViewData["Title"]</title>

    <link href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" type="text/css" rel="stylesheet" />
    <link asp-href-include="~/lib/bootstrap/**/*.min.css" type="text/css" rel="stylesheet" />
    <link asp-href-include="~/css/**/*.min.css" asp-append-version="true" type="text/css" rel="stylesheet" />
    <script src="~/lib/jquery/jquery.min.js" type="text/javascript"></script>
</head>

<body>
    <div class="container">
        @RenderBody()
    </div>

    <script src="~/lib/jquery-ajax-unobtrusive/dist/jquery.unobtrusive-ajax.min.js" type="text/javascript"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js" type="text/javascript"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
    <script src="~/js/localvalid.min.js" type="text/javascript"></script>
</body>
</html>
